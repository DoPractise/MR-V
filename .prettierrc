{
  "printWidth": 120,
  "tabWidth": 2, 
  "useTabs": false, 
  "semi": true, 
  "singleQuote": false, 
  "proseWrap": "preserve", 
  "arrowParens": "avoid", 
  "bracketSpacing": true,
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "ignore",
  "ignorePath": ".prettierignore",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "requireConfig": false,
  "trailingComma": "none"
}
