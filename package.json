{
  "name": "mrv",
  "version": "1.0.0",
  "description": "easy vue template",
  "author": "physeter <116091iyh@gmail.com>",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md,vue}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ],
    "src/**/*.{css,scss,less,json,html,md,markdown}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.24.0",
    "corejs": "^1.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "lib-flexible": "^0.3.2",
    "nprogress": "^0.2.0",
    "print-js": "^1.6.0",
    "vue": "^2.6.10",
    "vue-esign": "^1.1.0",
    "vue-pdf": "^4.3.0",
    "vue-router": "^3.0.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "autoprefixer": "^9.8.8",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-dynamic-import-node": "^2.3.3",
