    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "node-sass": "^5.0.0",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^2.5.0",
    "px2rem-loader": "^0.1.9",
    "sass-loader": "^10.0.5",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
