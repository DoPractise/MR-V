{
  "name": "mrv",
  "version": "1.0.0",
  "description": "easy vue template",
  "author": "physeter <116091iyh@gmail.com>",
  "private": true,
  "scripts": {
    "build": "webpack --config ./webpack.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md,vue}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ],
    "src/**/*.{css,scss,less,json,html,md,markdown}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.24.0",
    "gremlins.js": "^2.2.0",
    "html2canvas": "^1.4.1",
    "jquery": "^3.6.0",
    "js-cookie": "^3.0.1",
    "jspdf": "^2.5.1",
    "nprogress": "^0.2.0",
    "print-js": "^1.6.0",
    "qs": "^6.5.3",
    "vant": "^2.12.35",
    "vue-esign": "^1.1.0",
    "vue-pdf": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "babel-loader": "^8.2.5",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "webpack": "^4.44.0",
    "webpack-cli": "^4.10.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
